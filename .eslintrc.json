{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "standard",
    "standard-react",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1 
  },
  "globals": {
    "window": true,
    "document": true,
    "navigator": true,
    "fetch": true
  }
}